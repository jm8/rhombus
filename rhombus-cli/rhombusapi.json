{
  "openapi": "3.1.0",
  "info": { "title": "", "description": "an example API", "version": "" },
  "paths": {
    "/": {
      "get": {
        "responses": {
          "200": {
            "description": "plain text",
            "content": { "text/plain; charset=utf-8": {} }
          }
        }
      }
    },
    "/challenges": {
      "get": {
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/Challenge" }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Challenge": {
        "type": "object",
        "required": [
          "author",
          "category",
          "description",
          "files",
          "id",
          "name"
        ],
        "properties": {
          "author": { "type": "string" },
          "category": { "type": "string" },
          "description": { "type": "string" },
          "files": {
            "type": "array",
            "items": { "$ref": "#/components/schemas/ChallengeAttachment" }
          },
          "flag": { "type": ["string", "null"] },
          "healthscript": { "type": ["string", "null"] },
          "id": { "type": "string" },
          "name": { "type": "string" },
          "ticket_template": { "type": ["string", "null"] }
        }
      },
      "ChallengeAttachment": {
        "type": "object",
        "required": ["name", "url"],
        "properties": {
          "name": { "type": "string" },
          "url": { "type": "string" }
        }
      }
    }
  }
}
